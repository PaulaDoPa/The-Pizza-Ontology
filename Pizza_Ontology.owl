    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#PizzaToppings"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Country -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Country">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#America"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#England"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#France"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Germany"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Italy"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Pizzabase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#FourCheesesPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#FourCheesesPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


