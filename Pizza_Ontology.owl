        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasCountryOfOrigin"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Italy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
