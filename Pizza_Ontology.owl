     -->

    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#MozarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#MozarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that has mozarella and tomato (and basil!)</rdfs:comment>
    </owl:Class>
    


