<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#"
     xml:base="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10">
        <rdfs:comment>This is a practise ontology for class.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isToppingOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Pizzabase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasCountryOfOrigin"/>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasIngredient"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#PizzaToppings"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#hasCalorificContentValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
