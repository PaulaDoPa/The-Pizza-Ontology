    <!-- http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/9/untitled-ontology-10#CaperTopping">
